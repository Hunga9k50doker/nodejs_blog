<div class="mt-4">
    <h3>Khóa học của tôi</h3>
    <table class="table">
    <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Tên khóa học</th>
                <th scope="col">Trình độ</th>
                <th scope="col">Thời gian tạo</th>
            </tr>
    </thead>
    <tbody>
        {{#each courses}}
            <tr>
                <th scope="row">{{sum }}</th>
                <td>{{this.name}}</td>
                <td>{{this.level}}</td>
                <td>{{this.createAt}}</td>

            </tr>
       {{/each}}
    </tbody>
    </table>
</div>